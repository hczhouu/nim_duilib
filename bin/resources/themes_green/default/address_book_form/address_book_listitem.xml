<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<Window>
    <ListContainerElement class="listitem" name="list_all_user" width="200" height="64" mousechild="false">
        <Event type="select" receiver=".\headmask" applyattribute="selected=&quot;true&quot;"/>
        <Event type="unselect" receiver=".\headmask" applyattribute="selected=&quot;false&quot;"/>
        <Event type="select" receiver=".\checkbox" applyattribute="selected=&quot;true&quot;"/>
        <Event type="unselect" receiver=".\checkbox" applyattribute="selected=&quot;false&quot;"/>
        <Event type="mouseenter" receiver=".\headmask" applyattribute="state=&quot;hot&quot;"/>
        <Event type="mouseleave" receiver=".\headmask" applyattribute="state=&quot;normal&quot;"/>

        <HBox>
            <CheckBox class="circle_option" name="checkbox" valign="center" margin="18,0,0,0"/>

            <Box width="auto" height="auto" valign="center" margin="8,0,0,0">
                <Button name="head_image" width="46" height="46" margin="1,1,0,0" borderround="46,46,46,46"/>
                <CheckBox class="checkbox_headimage_mask_48x48" name="headmask"/>
				<Control name="logo" width="auto" height="auto" bkimage="logo.png" float="true" margin="28,28,0,0" visible="false"/>
            </Box>

            <VBox height="auto" valign="center" margin="8">
                <Label name ="show_name" font="2" normaltextcolor="darkcolor" width="stretch" valign="center"/>
                <Label name ="mobile" font="1" normaltextcolor="lightcolor" width="stretch" valign="center" margin="0,4,0,0"/>
            </VBox>
        </HBox>
    </ListContainerElement>
</Window>