<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<Window>
    <ListContainerElement class="listitem" name="list_all_user" width="200" height="75">
        <Event type="select" receiver=".\headmask" applyattribute="selected=&quot;true&quot;"/>
        <Event type="unselect" receiver=".\headmask" applyattribute="selected=&quot;false&quot;"/>
        <Event type="mouseenter" receiver=".\headmask" applyattribute="state=&quot;hot&quot;"/>
        <Event type="mouseleave" receiver=".\headmask" applyattribute="state=&quot;normal&quot;"/>
        <Event type="select" receiver=".\checkbox" applyattribute="selected=&quot;true&quot;"/>
        <Event type="unselect" receiver=".\checkbox" applyattribute="selected=&quot;false&quot;"/>
        
        <ButtonBox name="elem_btn_box" mousechild="false">
            <HBox>
                <CheckBox class="circle_option" name="checkbox" valign="center" margin="10,0,0,0"/>

                <Box width="auto" height="auto" valign="center" margin="10,0,0,0">
                    <Button name="head_image" width="38" height="38" margin="1,1,1,1" borderround="38,38,38,38"/>
                    <CheckBox name="headmask"/>
                </Box>

                <Label name ="show_name" font="2" normaltextcolor="darkcolor" width="auto" maxwidth="96" margin="10,0,2,0" valign="center"/>
                <Control name="icon_mute" bkimage="..\public\icon\icon_mute.png" width="auto" height="auto" visible="false" margin="0,3,0,0" valign="center"/>
            </HBox>
        </ButtonBox>
    </ListContainerElement>
</Window>