<?xml version="1.0" encoding="UTF-8"?>
<Window size="380,225" caption="0,0,0,34" text="移除群成员">
    <VBox bkcolor="bk_wnd_lightcolor" width="380" height="225">
        <HBox class="caption">
            <Label class="font_title" name="title" width="stretch" valign="center" normaltextcolor="darkcolor" text="移除群成员"/>
            <Button class="btn_wnd_close" name="closebtn" margin="4,6,0,0"/>
        </HBox>

        <Control />
        <VBox height="auto" padding="40,0,0,0">
            <Option class="circle_option_2" name="kick_user" group="remove_select" font="2" text="移除群成员"/>
            <Control height="15"/>
            <Option class="circle_option_2" name="kick_user_forbid" group="remove_select" font="2" text="移除并且禁止该成员再次进群"/>
        </VBox>
        <Control />

        <HBox height="30" margin="20,0,20,20">
            <Control name="left"/>
            <Button  class="btn_global_white_80x30" name="cancel" text="取消"/>
            <Control name="space_1_2" width="10"/>
            <Button  class="btn_global_green_80x30" name="confirm" text="确定"/>
        </HBox>
    </VBox>
</Window>
